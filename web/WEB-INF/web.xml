<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>control.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JNDIBindServlet</servlet-name>
        <servlet-class>control.JNDIBindServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JNDIRebindServlet</servlet-name>
        <servlet-class>control.JNDIRebindServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JNDISearchServlet</servlet-name>
        <servlet-class>control.JNDISearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JNDIUnbindServlet</servlet-name>
        <servlet-class>control.JNDIUnbindServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchUserListServlet</servlet-name>
        <servlet-class>control.SearchUserListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserListServlet</servlet-name>
        <servlet-class>control.UserListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowSelectedUserServlet</servlet-name>
        <servlet-class>control.ShowSelectedUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConcurrencyServlet</servlet-name>
        <servlet-class>control.ConcurrencyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryMessageServlet</servlet-name>
        <servlet-class>control.QueryMessageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentEJBServlet</servlet-name>
        <servlet-class>control.StudentEJBServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentWebServlet</servlet-name>
        <servlet-class>control.StudentWebServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JNDIBindServlet</servlet-name>
        <url-pattern>/JNDIBindServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JNDIRebindServlet</servlet-name>
        <url-pattern>/JNDIRebindServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JNDISearchServlet</servlet-name>
        <url-pattern>/JNDISearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JNDIUnbindServlet</servlet-name>
        <url-pattern>/JNDIUnbindServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchUserListServlet</servlet-name>
        <url-pattern>/SearchUserListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserListServlet</servlet-name>
        <url-pattern>/UserListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowSelectedUserServlet</servlet-name>
        <url-pattern>/ShowSelectedUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConcurrencyServlet</servlet-name>
        <url-pattern>/ConcurrencyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryMessageServlet</servlet-name>
        <url-pattern>/QueryMessageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentEJBServlet</servlet-name>
        <url-pattern>/StudentEJBServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentWebServlet</servlet-name>
        <url-pattern>/StudentWebServlet</url-pattern>
    </servlet-mapping>
</web-app>